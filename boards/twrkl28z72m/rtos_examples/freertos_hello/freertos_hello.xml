<?xml version="1.0" encoding="UTF-8"?>
<ksdk:examples xmlns:ksdk="http://nxp.com/ksdk/2.0/ksdk_manifest_v3.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://nxp.com/ksdk/2.0/ksdk_manifest_v3.0.xsd manifest.xsd">
  <externalDefinitions>
    <!-- Toolchain options to be overwritten -->
    <definition extID="com.nxp.mcuxpresso"/>
    <definition extID="middleware.freertos.MKL28Z7"/>
    <definition extID="middleware.freertos.heap_4.MKL28Z7"/>
    <definition extID="platform.devices.MKL28Z7_CMSIS.MKL28Z7"/>
    <definition extID="platform.devices.MKL28Z7_startup.MKL28Z7"/>
    <definition extID="platform.drivers.clock.MKL28Z7"/>
    <definition extID="platform.drivers.common.MKL28Z7"/>
    <definition extID="platform.drivers.flash.MKL28Z7"/>
    <definition extID="platform.drivers.gpio.MKL28Z7"/>
    <definition extID="platform.drivers.lpuart.MKL28Z7"/>
    <definition extID="platform.drivers.port.MKL28Z7"/>
    <definition extID="platform.drivers.smc.MKL28Z7"/>
    <definition extID="platform.utilities.debug_console.MKL28Z7"/>
    <definition extID="iar"/>
    <definition extID="mdk"/>
    <definition extID="armgcc"/>
    <definition extID="kds"/>
    <definition extID="mcuxpresso"/>
    <definition extID="drt"/>
  </externalDefinitions>
  <example id="twrkl28z72m_rtos_examples_freertos_hello" name="freertos_hello" category="rtos_examples" dependency="platform.drivers.lpuart.MKL28Z7 platform.drivers.port.MKL28Z7 platform.drivers.clock.MKL28Z7 middleware.freertos.heap_4.MKL28Z7 platform.drivers.common.MKL28Z7 platform.utilities.debug_console.MKL28Z7 platform.devices.MKL28Z7_CMSIS.MKL28Z7 middleware.freertos.MKL28Z7 platform.drivers.gpio.MKL28Z7 platform.drivers.smc.MKL28Z7 platform.drivers.flash.MKL28Z7 platform.devices.MKL28Z7_startup.MKL28Z7" toolchain="iar mdk armgcc kds mcuxpresso drt">
    <projects>
      <project type="com.crt.advproject.projecttype.exe" nature="org.eclipse.cdt.core.cnature"/>
    </projects>
    <toolchainSettings>
      <!-- Curently, mcux just supports one common settings set for both debug and release -->
      <toolchainSetting id_refs="com.nxp.mcuxpresso">
        <!-- asm compiler -->
        <option id="com.crt.advproject.gas.arch" type="enum">
          <value>com.crt.advproject.gas.target.cm0plus</value>
        </option>
        <option id="com.crt.advproject.gas.fpu" type="enum">
          <value>com.crt.advproject.gas.fpu.none</value>
        </option>
        <option id="gnu.both.asm.option.warnings.nowarn" type="boolean">
          <value>false</value>
        </option>
        <!-- gcc compiler -->
        <option id="com.crt.advproject.gcc.arch" type="string">
          <value>com.crt.advproject.gcc.target.cm0plus</value>
        </option>
        <option id="com.crt.advproject.gcc.fpu" type="string">
          <value>com.crt.advproject.gcc.fpu.none</value>
        </option>
        <option id="com.crt.advproject.c.misc.dialect" type="enum">
          <value>com.crt.advproject.misc.dialect.gnu99</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.debug.option.debugging.level" type="enum">
          <value>gnu.c.debugging.level.default</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.release.option.debugging.level" type="enum">
          <value>gnu.c.debugging.level.default</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.debug.option.optimization.level" type="enum">
          <value>gnu.c.optimization.level.optimize</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.release.option.optimization.level" type="enum">
          <value>gnu.c.optimization.level.optimize</value>
        </option>
        <option id="gnu.c.compiler.option.warnings.nowarn" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.compiler.option.warnings.allwarn" type="boolean">
          <value>true</value>
        </option>
        <option id="gnu.c.compiler.option.warnings.extrawarn" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.compiler.option.warnings.wconversion" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.compiler.option.warnings.toerrors" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.compiler.option.preprocessor.def.symbols" type="stringList">
          <value>DEBUG</value>
          <value>CPU_MKL28Z512VLL7</value>
          <value>FSL_RTOS_FREE_RTOS</value>
          <value>TWR_KL28Z72M</value>
          <value>TOWER</value>
        </option>
        <option id="gnu.c.compiler.option.optimization.flags" type="string">
          <value>-fno-common</value>
        </option>
        <option id="gnu.c.compiler.option.misc.other" type="enum">
          <value>-c  -ffunction-sections  -fdata-sections  -ffreestanding  -fno-builtin</value>
        </option>
        <option id="gnu.c.compiler.option.preprocessor.nostdinc" type="boolean">
          <value>false</value>
        </option>
        <!-- gcc linker -->
        <option id="com.crt.advproject.link.arch" type="enum">
          <value>com.crt.advproject.link.target.cm0plus</value>
        </option>
        <option id="com.crt.advproject.link.fpu" type="enum">
          <value>com.crt.advproject.link.fpu.none</value>
        </option>
        <option id="gnu.c.link.option.nostart" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.link.option.nodeflibs" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.link.option.nostdlibs" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.link.option.strip" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.link.option.noshared" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.link.option.other" type="stringList">
          <value>--gc-sections</value>
        </option>
      </toolchainSetting>
    </toolchainSettings>
    <source path="boards/twrkl28z72m/rtos_examples/freertos_hello" target_path="board" type="c_include">
      <files mask="board.h"/>
      <files mask="clock_config.h"/>
      <files mask="pin_mux.h"/>
    </source>
    <source path="boards/twrkl28z72m/rtos_examples/freertos_hello" target_path="board" type="src">
      <files mask="board.c"/>
      <files mask="clock_config.c"/>
      <files mask="pin_mux.c"/>
    </source>
    <source path="boards/twrkl28z72m/rtos_examples/freertos_hello" target_path="doc" type="doc">
      <files mask="readme.txt"/>
    </source>
    <source path="boards/twrkl28z72m/rtos_examples/freertos_hello" target_path="source" type="c_include">
      <files mask="FreeRTOSConfig.h"/>
    </source>
    <source path="boards/twrkl28z72m/rtos_examples/freertos_hello" target_path="source" type="src">
      <files mask="freertos_hello.c"/>
    </source>
  </example>
</ksdk:examples>
